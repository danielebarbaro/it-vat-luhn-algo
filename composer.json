{
    "name": "danielebarbaro/it-vat-luhn-algo",
    "homepage": "https://github.com/danielebarbaro/it-vat-luhn-algo",
    "description": "Just a funny test to validate IT VAT",
    "license": "MIT",
    "keywords": [
        "danielebarbaro",
        "vat-it-validator",
        "vat-validator",
        "vat"
    ],
    "authors": [
        {
            "name": "Daniele Barbaro",
            "email": "barbaro.daniele@gmail.com",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "8.5.x-dev",
        "squizlabs/php_codesniffer": "^3.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit tests",
        "test-coverage": "vendor/bin/phpunit tests --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    }
}
